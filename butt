#! /bin/sh

print_pass() {
  :
}

print_fail() {
  echo -n "f "
}

runtest() {
  filename=$1
  "$filename"
  success=$?
  if [ $success -eq 0 ]; then
    print_pass
  else
    print_fail
    echo "$filename"
  fi
  return $success
}

fail=0

for filename in ./test*; do
  if [ -d "$filename" ]; then
    continue
  fi
  runtest "$filename" || fail=1
done

exit $fail
